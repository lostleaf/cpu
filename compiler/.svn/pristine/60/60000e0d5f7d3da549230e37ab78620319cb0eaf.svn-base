package roxanne.types;

public final class ARRAY extends POINTER {
	public Integer capacity;
	public ARRAY() { super(); capacity = null; }
	public ARRAY(TYPE t, Integer i) { super(t); capacity = i; }
	public TYPE actual() {
		if (capacity!=null && capacity>=0 && !(super.actual() instanceof WRONG)) return this;
		return new WRONG();
	}
	public boolean equals(ARRAY a) {
		return super.equals(a);
	}
	public String toString() {
		return "("+super.toString()+"<-"+capacity.toString()+")";
	}
}
